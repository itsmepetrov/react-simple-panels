{
  "extends": "eslint-config-airbnb",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2,
    "indent": [2, 4],
    "comma-dangle": 0,
    "no-shadow": 0
  },
  "plugins": [
    "react"
  ],
  "parser": "babel-eslint"
}